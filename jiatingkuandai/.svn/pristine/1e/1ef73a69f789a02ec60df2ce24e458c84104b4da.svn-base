       		  }
        	}else{
        		alert("上月运营商占比饼图--数据源为空"); 
        	}
   		  
          pieChart.Doughnut(PieData, pieOptions);
        },  
        error : function(errorMsg) {  
            alert("上月运营商占比饼图--返回数据出错");  
        }  
    });
	//拼上月运营商明细数据源  
	    function appendTable(result,tableId){
		  var jsonData = [];
