package com.qualitymap.action;

import javax.annotation.Resource;

import com.qualitymap.base.BaseAction;
import com.qualitymap.service.OverviewKPIOrgidBandwidthService;


public class OverviewKPIOrgidBandwidthAction extends BaseAction {

	/**
	 * serialVersionUID long OverviewKpiAction.java
	 */
	private static final long serialVersionUID = 1L;

	@Resource
	private OverviewKPIOrgidBandwidthService overviewKPIOrgidBandwidthService;

	/**
	 * 根据groupid，month获取样本总数
	 * 
	 * @return void
	 */
	public void getTotalSampleNum() {
		try {
			String month = this.servletRequest.getParameter("month");
			String groupid = this.servletRequest.getParameter("groupid");  //页面获取指定groupid
			String new_sample_num = overviewKPIOrgidBandwidthService.getTotalSampleNum(month, groupid);
			printWriter(new_sample_num);
		} catch (Exception e) {
			// TODO: handle exception
			e.printStackTrace();
		}
	}
}
