<script>
  $(function () {
	$("#example1").DataTable({
      "paging": false,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false
    });
    $('#example2').DataTable({
      "paging": false,
      "lengthChange": false,
      "searching": false,
      "ordering": true,
      "info": true,
      "autoWidth": false
    });

    //-------------
    //- PIE CHART -
    //-------------
    var pieChartCanvas = $("#pieChart").get(0).getContext("2d");
    var pieChart = new Chart(pieChartCanvas);
    var PieData = [];
    var pieOptions = {
      //Boolean - Whether we should show a stroke on each segment
      segmentShowStroke: true,
      //String - The colour of each segment stroke
      segmentStrokeColor: "#fff",
      //Number - The width of each segment stroke
      segmentStrokeWidth: 2,
      //Number - The percentage of the chart that we cut out of the middle
      percentageInnerCutout: 50, // This is 0 for Pie charts
      //Number - Amount of animation steps
      animationSteps: 100,
      //String - Animation easing effect
      animationEasing: "easeOutBounce",
      //Boolean - Whether we animate the rotation of the Doughnut
      animateRotate: true,
      //Boolean - Whether we animate scaling the Doughnut from the centre
      animateScale: false,
      //Boolean - whether to make the chart responsive to window resizing
      responsive: true,
      // Boolean - whether to maintain the starting aspect ratio or not when responsive, if set to false, will take up entire container
      maintainAspectRatio: true,
      //String - A legend template
      legendTemplate: "<ul class=\"<%=name.toLowerCase()%>-legend\"><% for (var i=0; i<segments.length; i++){%><li><span style=\"background-color:<%=segments[i].fillColor%>\"></span><%if(segments[i].label){%><%=segments[i].label%><%}%></li><%}%></ul>"
    };
    //pieChart.Doughnut(PieData, pieOptions);

    //-------------
    //- BAR CHART -
    //-------------
    var barChartCanvas = $("#barChart").get(0).getContext("2d");
    var barChart = new Chart(barChartCanvas);
    var barChartData =  [];
    
    //barChart.Doughnut(barChartData, pieOptions);
////////////////////////// 上月运营商占比 start/////////////////
    //chart
    
    $.ajax({  
        type : "post",  
        async : false, //同步执行  
        url:'operatorqualitymap/getOperatorDatas',
        data : {month:yearMonth},  
        dataType : "json", //返回数据形式为json  
        success : function(result) {  
   		  for(var j=0;j<result.data.length;j++){
   				var jsonObj={value:"",color:"",highlight:"",label:""};
   				jsonObj.value=result.data[j].newUserNum;
   				jsonObj.color=pie_color[j].color;
   				jsonObj.highlight=pie_color[j].highlight;
   				jsonObj.label=result.data[j].operator;
   				PieData.push(jsonObj);
   		  }
          pieChart.Doughnut(PieData, pieOptions);
        },  
        error : function(errorMsg) {  
            alert("返回数据出错");  
        }  
    });
  //拼测试报告tableBody
    function appendTable(result){
  	  for(var i=0;i<result.data.length;i++){
  		  $("#example1 tbody").append(
  				  "<tr>"+
  				  	"<td>"+result.data[i].operator+"</td>"+
  				  	"<td>"+result.data[i].newUserNum+"</td>"+
  				  	"<td>"+result.data[i].proportion+"</small>%<small></td>"+
  				  	"<td>"+result.data[i].accumulativ_num+"</td>"+
  				  	"<td>"+result.data[i].newly_increase_num+"</td>"+
  				  "</tr>"
  		  );
  	  }
    }
    //明细
    $.ajax({  
        type : "post",  
        async : false, //同步执行  
        url:'operatorqualitymap/getDetailData',
        data : {month:yearMonth},  
        dataType : "json", //返回数据形式为json  
        success : function(result) {  
        	if(result.data.length>0){
      		  	appendTable(result);
      	  	}else{
      		  	alert("没有数据记录！");
      	  	}
        },  
        error : function(errorMsg) {  
            alert("返回数据出错");  
        }  
    });
//////////////////////////上月运营商占比 end /////////////////

//////////////////////////上月联网方式占比 start /////////////////
  //拼测试报告tableBody
    function appendTable2(result){
  	  for(var i=0;i<result.data.length;i++){
  		  $("#example2 tbody").append(
  				  "<tr>"+
  				  	"<td>"+result.data[i].net_type+"</td>"+
  				  	"<td>"+result.data[i].newUserNum+"</td>"+
  				  	"<td>"+result.data[i].proportion+"</small>%<small></td>"+
  				  	"<td>"+result.data[i].accumulativ_num+"</td>"+
  				  	"<td>"+result.data[i].newly_increase_num+"</td>"+
  				  "</tr>"
  		  );
  	  }
    }
    $.ajax({  
        type : "post",  
        async : false, //同步执行  
        url:'qualitymap/getNetTypeData',
        data : {month:yearMonth},  
        dataType : "json", //返回数据形式为json  
        success : function(result) {  
        	if(result.data.length>0){
        		for(var i=0;i<result.data.length;i++){
        			var jsonObj={value:"",color:"",highlight:"",label:""};
       				jsonObj.value=result.data[i].newUserNum;
       				jsonObj.color=pie_color[i].color;
       				jsonObj.highlight=pie_color[i].highlight;
       				jsonObj.label=result.data[i].net_type;
       				barChartData.push(jsonObj);
        			//barChartData[i].label = result.data[i].net_type;
        			//barChartData[i].value = result.data[i].proportion;
            	}
        		appendTable2(result);
      	  	}else{
      		  	alert("数据量不匹配或者没有数据记录！");
      	  	}
        	barChart.Doughnut(barChartData, pieOptions);
        },  
        error : function(errorMsg) {  
            alert("返回数据出错");  
        }  
    });
//////////////////////////上月联网方式占比 end /////////////////



  });
</script>
